{
  "name": "ide-YOUR-LANGUAGE-NAME",
  "main": "./lib/AtomPackageLSPMain",
  "version": "1.0.0",
  "description": "Language support for Atom-IDE.",
  "license": "MIT",
  "engines": {
    "atom": ">=1.21.0"
  },
  "dependencies": {
    "atom-languageclient": "0.9.9"
  },
  "enhancedScopes": [
    "YOUR-LANGUAGE-NAME"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "console": {
      "versions": {
        "0.1.0": "consumeConsole"
      }
    },
    "signature-help": {
      "versions": {
        "0.1.0": "consumeSignatureHelp"
      }
    },
    "atom-ide-busy-signal": {
      "versions": {
        "0.1.0": "consumeBusySignal"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideRangeCodeFormat"
      }
    },
    "code-format.onType": {
      "versions": {
        "0.1.0": "provideOnTypeCodeFormat"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  }
}
